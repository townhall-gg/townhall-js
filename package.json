{
  "name": "townhall-js",
  "version": "0.0.0",
  "private": true,
  "description": "Official JavaScript/TypeScript SDK for TownHall form submissions",
  "repository": {
    "type": "git",
    "url": "https://github.com/townhall-gg/townhall-js.git"
  },
  "author": "TownHall <hello@townhall.gg>",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
